# .editorconfig for C#
root = true

[*.cs]
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 120

csharp_new_line_before_open_brace = all
csharp_new_line_after_keywords = if, else, for, foreach, while, do, switch, try, catch, finally
csharp_style_var_for_built_in_types = true:suggestion
